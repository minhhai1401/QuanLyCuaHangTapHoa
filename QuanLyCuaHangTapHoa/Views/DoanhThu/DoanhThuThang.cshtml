    }

    @@keyframes pulseWarning {
        0% {
            transform: scale(1);
            box-shadow: 0 0 0 0 rgba(220, 38, 38, 0.7);
        }

        70% {
            transform: scale(1.05);
            box-shadow: 0 0 0 10px rgba(220, 38, 38, 0);
        }

        100% {
            transform: scale(1);
            box-shadow: 0 0 0 0 rgba(220, 38, 38, 0);
        }
    }

    .warning-icon i {
        font-size: 2rem;
        color: white;
    }

    .modal-title {
        color: #991b1b;
        font-size: 1.5rem;
        font-weight: 700;
        margin: 0;
    }

    .modal-body {
        padding: 2rem;
        text-align: center;
    }

    .warning-message {
        font-size: 1.1rem;
        color: #1f2937;
        margin-bottom: 0.5rem;
        font-weight: 600;
    }

    .warning-detail {
        color: #6b7280;
        margin-bottom: 1.5rem;
    }

    .order-preview {
        background: #f3f4f6;
