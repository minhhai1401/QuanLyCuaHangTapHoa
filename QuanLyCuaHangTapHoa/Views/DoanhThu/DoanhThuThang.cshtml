    }

    .order-id {
        font-size: 1.25rem;
        font-weight: 600;
        color: #374151;
    }

    .confirmation-input {
        margin-top: 1.5rem;
        position: relative;
    }

    .confirm-instruction {
        margin-bottom: 0.75rem;
