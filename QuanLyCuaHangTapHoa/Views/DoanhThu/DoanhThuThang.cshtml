    }

        .btn-confirm:not(:disabled) {
            opacity: 1;
            cursor: pointer;
        }

            .btn-confirm:not(:disabled):hover {
                background: #b91c1c;
                transform: translateY(-2px);
                box-shadow: 0 4px 6px -1px rgba(220, 38, 38, 0.3);
            }

